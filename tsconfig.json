{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "lib": ["es2023"],
    "module": "ESNext",
    "target": "es2022",
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
  },
    "include": ["./**/*"]
}
